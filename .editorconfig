# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
# Unix-style newlines with a newline ending every
end_of_line = lf
insert_final_newline = true
# Set default indentation
indent_style = space
indent_size = 2
# Set default charset
charset = utf-8
# Clean up whitespace
trim_trailing_whitespace = true
